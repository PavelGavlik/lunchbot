{
  "name": "prague-lunchbot",
  "version": "1.0.0",
  "engines": {
    "node": "^18.0.0",
    "yarn": "^1.12.3"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/aliceTechnologies.ts",
    "test": "ts-node ./node_modules/.bin/mocha **/*.test.ts",
    "test:watch": "yarn test --watch",
    "test:coverage": "nyc npm run test"
  },
  "author": "Pavel Gavl√≠k",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^3.2.6",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/mocha": "^9.1.1",
    "@types/node-fetch": "^2.3.3",
    "mocha": "^10.0.0",
    "nyc": "15.1.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "lines": 50,
    "all": true,
    "check-coverage": true,
    "include": "**/*.ts",
    "exclude": "**/*.test.ts",
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
