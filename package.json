{
  "name": "prague-lunchbot",
  "version": "1.0.0",
  "engines": {
    "node": "^12.6.0 || ^13.0.0",
    "yarn": "^1.12.3"
  },
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/aliceTechnologies.ts",
    "test": "mocha **/*.test.ts",
    "test:watch": "yarn test --watch",
    "test:coverage": "nyc npm run test"
  },
  "author": "Pavel Gavl√≠k",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^2.6.7",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/mocha": "^5.2.7",
    "@types/node-fetch": "^2.3.3",
    "mocha": "^6.2.0",
    "nyc": "13.2.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "lines": 50,
    "all": true,
    "check-coverage": true,
    "include": "**/*.ts",
    "exclude": "**/*.test.ts",
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ]
  }
}
